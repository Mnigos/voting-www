  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "pnpm format && pnpm lint:js",
